{
  "title": "MyCCA API SLOs",
  "panels": [
    {
      "type": "graph",
      "title": "API p95 Latency (ms)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "graph",
      "title": "API Error Rate (5xx/4xx)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..|4..\"}[5m]))",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    }
  ]
}